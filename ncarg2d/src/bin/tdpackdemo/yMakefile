MYNAME  = tdpackdemo

SRC    = TdpackDemo.f
OBJ    = TdpackDemo.o

PROG_LIBS =	$(NCARGLIBS) $(PNGLIB) $(CAIROLIB)
DEP_LIBS =	$(DEPNCARGLIBS)
SYS_LIBS	= $(XLIB)

#if defined(Darwin)
#EXTRA_FCOPTIONS  = -N11
#endif

FortranProgram($(MYNAME),$(OBJ),$(DEP_LIBS))
InstallTarget($(SRC),$(INSTALL_LIB),$(EXAMPLESPATH))

